# 測試環境配置

# 應用程式設定
VITE_APP_TITLE=廣清宮財務管理系統 (測試版)
VITE_APP_VERSION=1.0.0-staging
VITE_APP_DESCRIPTION=專為宮廟設計的財務管理系統 - 測試環境

# 環境設定
NODE_ENV=production
VITE_MODE=staging

# API 設定
VITE_API_BASE_URL=https://staging-api.guangqing-temple.com
VITE_API_TIMEOUT=15000

# 功能開關
VITE_ENABLE_MOCK=false
VITE_ENABLE_DEVTOOLS=true
VITE_ENABLE_CONSOLE=true
VITE_ENABLE_PERFORMANCE_MONITOR=true
VITE_ENABLE_ERROR_REPORTING=true

# 第三方服務
VITE_SENTRY_DSN=your-staging-sentry-dsn-here
VITE_GOOGLE_ANALYTICS_ID=your-staging-ga-id-here

# 安全設定
VITE_ENABLE_HTTPS=true
VITE_SECURE_COOKIES=true

# 快取設定
VITE_CACHE_VERSION=1.0.0-staging
VITE_ENABLE_SERVICE_WORKER=false

# 部署設定
VITE_PUBLIC_PATH=/
VITE_BUILD_SOURCEMAP=true
VITE_BUILD_MINIFY=true
